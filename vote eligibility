#include <iostream>
using namespace std;

int main()
{
    int age;
    cout << "Enter your Age to check if you are eligible to vote or not : ";
    cin >> age;
    if (age >= 18)
    {
        cout << "You are eligible to vote!";
    }
    else if (age <= 0)
    {
        cout << "Age Entered is Invalid!";
    }
    else
    {
        cout << "You are not eligible to vote!";
    }

    return 0;
}
